{
  "name": "vs-website",
  "version": "0.0.1",
  "description": "valor-software.com",
  "main": "index.js",
  "scripts": {
    "flow.clean": "del bundles coverage dist",
    "deploy:gh-pages": "npm run flow.clean && npm run flow.build:prod && sh ./deploy.sh",
    "manual.deploy:gh-pages": "npm run flow.clean && npm run flow.build:prod && gh-pages -d dist -r git@github.com:VS-work/VS-work.github.io.git -b master -m 'manual deploy to GitHub Pages'",
    "flow.eslint": "eslint --ignore-path .gitignore --ext js --fix . .config",
    "flow.tslint": "gulp lint",
    "flow.lint": "npm run flow.tslint",
    "flow.changelog": "conventional-changelog -i CHANGELOG.md -s -p angular -v",
    "flow.github-release": "conventional-github-releaser -p angular",
    "flow.build:prod": "ng build --prod -aot",
    "flow.build:dev": "ng build -aot",
    "flow.serve:dev": "ng serve -aot",
    "flow.serve:prod": "ng serve --prod -aot",
    "start": "npm run flow.serve:dev",
    "pretest": "npm run flow.lint",
    "test": "NODE_ENV=test echo 'karma start'",
    "test:watch": "NODE_ENV=test karma start --auto-watch --no-single-run",
    "preversion": "npm test",
    "version": "npm run flow.changelog && git add -A",
    "postversion": "git push origin development && git push --tags"
  },
  "author": "Oleksandr Telnov <otelnov@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/VS-work/vs-website.git"
  },
  "bugs": {
    "url": "https://github.com/VS-work/vs-website/issues"
  },
  "homepage": "https://valor-software.com",
  "devDependencies": {
    "@types/jasmine": "2.5.35",
    "@types/node": "6.0.45",
    "@types/webpack": "1.12.35",
    "codecov": "1.0.1",
    "codelyzer": "0.0.28",
    "conventional-changelog": "1.1.0",
    "conventional-github-releaser": "1.1.3",
    "css-loader": "0.25.0",
    "del-cli": "0.2.0",
    "eslint-config-valorsoft": "0.1.0",
    "file-loader": "0.9.0",
    "gh-pages": "0.11.0",
    "gitignore-to-glob": "0.2.1",
    "gulp": "3.9.1",
    "gulp-tslint": "6.1.2",
    "ng2-webpack-config": "0.0.5",
    "pre-commit": "1.1.3",
    "style-loader": "0.13.1",
    "systemjs-builder": "0.15.32",
    "tslint-config-valorsoft": "1.1.1",
    "typescript": "2.0.3",
    "url-loader": "0.5.7",
    "angular-cli": "1.0.0-beta.18",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "4.0.9",
    "ts-node": "1.6.0",
    "tslint": "3.13.0"
  },
  "dependencies": {
    "@angular/common": "2.1.1",
    "@angular/compiler": "2.1.1",
    "@angular/core": "2.1.1",
    "@angular/forms": "2.1.1",
    "@angular/http": "2.1.1",
    "@angular/platform-browser": "2.1.1",
    "@angular/platform-browser-dynamic": "2.1.1",
    "@angular/router": "3.1.1",
    "core-js": "2.4.1",
    "ng2-bootstrap": "1.1.14",
    "ng2-page-scroll": "3.2.0",
    "particles.js": "2.0.0",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.26"
  }
}

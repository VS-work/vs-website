{
  "extends": "tslint-config-valorsoft",
  "rulesDirectory": "./node_modules/codelyzer",
  "rules": {
    "no-null-keyword": false,
    "only-arrow-functions": false,
    "max-file-line-count": [
      true,
      1000
    ],
    "no-parameter-properties": false,
    "member-access": [
      true,
      "check-accessor"
    ],
    "directive-selector": [
      true,
      "attribute",
      "",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "",
      "kebab-case"
    ],
    "pipe-naming": [
      true,
      "camelCase",
      ""
    ]
  }
}
